// Question 1
	private static void name(){
	System.out.println("---Question 1 ---");
	System.out.print("Belal");
	System.out.print("\n");
	
	}

// Question 2
	private static void sum(int a, int b) {
		int sum = a + b;
		System.out.println("---Question 2---");
		System.out.println("\n"+"Answer : " + sum);
		
	}

// Question 3
	private static void div (int a, int b) {
		int div = a / b;
		System.out.println("---Question 3---");
		System.out.println("Answer : " + div);
		
	}
	
	// Question 4
	private static void mixed () {
		System.out.println("---Question 4---");
		System.out.println("Answer : " + (-5 + 8) * 6);
		System.out.println("Answer : " + (55 + 9) % 9);
		System.out.println("Answer : " + (20 + -3*5 / 8));
		System.out.println("Answer : " + (5 + 15 / 3 * 2 - 8 % 3));
		}
	
	// Question 5
	private static void displayOutput (int a, int b) {
		System.out.println("---Question 5---");
		int sum = a * b;
		System.out.println("25 x 5 = " + (sum));
		}
	
	// Question 5 re-done
	private static void displayOutputv2 () {
		System.out.println("---Question 5 Re-done ---");
	
		// scanner
		Scanner dO = new Scanner(System.in);
		int sum, a, b;
		System.out.println("Enter first number");
		a = dO.nextInt();
		
		System.out.println("Enter second number");
		b = dO.nextInt();
		
		dO.close();
		
		sum = a * b;
		System.out.println(a + "x" + b + "=" + sum);
		}
	
	// Question 6
	private static void six() {
		System.out.println("---Question 6 ---");
	
		// scanner
		Scanner mT = new Scanner(System.in);
		int a, b;
		System.out.println("Enter number");
		a = mT.nextInt();
		
		System.out.println("Enter number");
		b = mT.nextInt();
		
		mT.close();
		
		System.out.printf("%d + %d = %d \n", a, b, a+b);
		System.out.printf("%d - %d = %d \n", a, b, a-b);
		System.out.printf("%d x %d = %d \n", a, b, a*b);
		System.out.printf("%d / %d = %d \n", a, b, a/b);
		System.out.printf("%d mod %d = %d \n", a, b, a%b);
		}
	
	// Question 7
	private static void multTable() {
		System.out.println("---Question 7 ---");
	
		// scanner
		Scanner mT = new Scanner(System.in);
		int a;
		System.out.println("Enter number");
		a = mT.nextInt();
		
		mT.close();
		
		for(int i = 1; i<= 10; ++i)
		{
			System.out.printf("%d * %d = %d \n", a, i, a*i);
			
		}
		
		}
	//Question 8
	private static void pictorial() {
		System.out.println("---Question 8 ---");

			System.out.printf("   J    a   v     v  a  \n");
			System.out.printf("   J   a a   v   v  a a \n");
			System.out.printf("J  J  aaaaa   V V  aaaaa \n");
			System.out.printf(" JJ  a     a   V  a     a \n");
	}
	
	//Question 9

	 private static void nine() {
		 System.out.println(((25.5 * 3.5 - 3.5 * 3.5) / (40.5 - 4.5)));


	 }
		}

