package basicPart1;

public class oneToTen {
	
public static void main(String[] args) {
	name();
	sum(1,2);
	}

// Question 1
	private static void name(){
	System.out.print("Belal Ali");
	System.out.print("Belal Ali");
	}

// Question 2
	private static void sum(int a, int b) {
		
		int sum = a + b;
		System.out.println(sum);
		
	}
	
	
	
	
	
	
}
